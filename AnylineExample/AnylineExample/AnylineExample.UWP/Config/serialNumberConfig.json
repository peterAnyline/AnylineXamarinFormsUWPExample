{
  "camera": {
    "captureResolution": "1080p",
    "pictureResolution": "Photo"
  },
  "flash": {
    "mode": "manual",
    "alignment": "top_right",
    "imageOn": "torch_on",
    "imageOff": "torch_off",
    "imageAuto": "torch_auto"
  },
  "viewPlugin": {
    "plugin": {
      "id": "METER",
      "meterPlugin": {
        "scanMode": "SERIAL_NUMBER"
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "width": 800,
      "alignment": "top_half",
      "ratioFromSize": {
        "width": 5,
        "height": 1
      },
      "strokeWidth": 2,
      "strokeColor": "FFFFFF",
      "cornerRadius": 4,
      "outerColor": "000000",
      "outerAlpha": 0.5,
      "feedbackStrokeColor": "0099FF",
      "maxWidthPercent": "80%",
      "maxHeightPercent": "80%",
      "offset": {
        "x": 0,
        "y": 0
      },
      "cropPadding": {
        "x": 0,
        "y": 0
      },
      "cropOffset": {
        "x": 0,
        "y": 0
      }
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "strokeColor": "0099FF",
      "strokeWidth": 2,
      "fillColor": "220099FF",
      "cornerRadius": 2,
      "redrawTimeout": 200,
      "animationDuration": 75,
      "animation": "NONE",
      "blinkOnResult": true,
      "beepOnResult": true,
      "vibrateOnResult": true,
      "cancelOnResult": true
    }
  }
}