{
  "camera": {
    "captureResolution": "2448",
    "resolutionRatio": {
      "numerator": 4,
      "denominator": 3
    }
  },
  "flash": {
    "mode": "manual",
    "alignment": "top_right",
    "imageOn": "torch_on",
    "imageOff": "torch_off",
    "imageAuto": "torch_auto"
  },
  "viewPlugin": {
    "plugin": {
      "id": "METER",
      "meterPlugin": {
        "scanMode": "PHOTO_CAPTURE"
      }
    },
    "cutoutConfig": {
      "style": "rect",
      "alignment": "top_half",
      "ratioFromSize": {
        "width": 125,
        "height": 200
      },
      "strokeWidth": 2,
      "strokeColor": "FFFFFF",
      "cornerRadius": 4,
      "outerColor": "000000",
      "outerAlpha": 0.0,
      "feedbackStrokeColor": "0099FF",
      "maxWidthPercent": "25%",
      "maxHeightPercent": "45%",
      "offset": {
        "x": 0,
        "y": 0
      },
      "cropPadding": {
        "x": 0,
        "y": 0
      },
      "cropOffset": {
        "x": 0,
        "y": 0
      }
    },
    "scanFeedback": {
      "style": "CONTOUR_RECT",
      "strokeColor": "FFFFFF",
      "strokeWidth": 2,
      "fillColor": "220099FF",
      "cornerRadius": 4,
      "redrawTimeout": 200,
      "animationDuration": 75,
      "animation": "NONE",
      "blinkOnResult": true,
      "beepOnResult": false,
      "vibrateOnResult": true,
      "cancelOnResult": true
    }
  }
}